doctype html
html.no-js(lang='en-US')
  head
    title #{title}
    meta(charset='UTF-8')
    meta(content='width=device-width, initial-scale=1', name='viewport')
    link(href='//fonts.googleapis.com/', rel='dns-prefetch')
    link#genericons-css(rel='stylesheet', href='/contrib/css/genericons/genericons.css')
    link#wm-google-fonts-css(href='//fonts.googleapis.com/css?family=Playfair+Display%7CDroid+Serif&subset&ver=2.2.0', rel='stylesheet')
    link#wm-main-css(href='/contrib/css/main.css', rel='stylesheet')
    link(rel='stylesheet', href='/css/style.css')
    link#wm-stylesheet-css(href='/contrib/css/style.css', rel='stylesheet')
    script(src='/contrib/js/jquery/jquery.js', type='text/javascript')
  body#top.has-post-thumbnail.home.is-singular.page.page-id-4.page-template.page-template-_fullwidth.page-template-page-template.page-template-page-template_fullwidth-php
    #page.hfeed.site
      header#masthead.site-header(itemscope='', itemtype='https://schema.org/WPHeader', role='banner')
        .site-header-inner
          .site-branding
            h1#site-title.site-title.logo.type-text
              span.text-logo Reservation App
          nav#site-navigation.main-navigation(aria-labelledby='site-navigation-label', itemscope='', itemtype='https://schema.org/SiteNavigationElement', role='navigation')
            h2#site-navigation-label.screen-reader-text Primary Menu
            #main-navigation-inner.main-navigation-inner
              .menu
                ul
                  li#menu-item-75.menu-item.menu-item-type-custom.menu-item-object-custom.menu-item-75
                    a(href='/') Home
                  li#menu-item-76.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-76
                    a(href='/reservations') Reservations
        button#menu-toggle.menu-toggle(aria-controls='site-navigation', aria-expanded='false') Menu
    #site-banner.site-banner.no-slider
      aside.site-banner-inner
        h2.screen-reader-text Site banner
        .site-banner-content
          .site-banner-media
            figure.site-banner-thumbnail
              img(alt='', height='686', src='/images/header.jpg', width='1640')
          .site-banner-header
            h3.entry-title.site-banner-title
              span.highlight
    
    
    // /#page
    script(src='/contrib/js/imagesloaded.min.js', type='text/javascript')
    script(src='/contrib/js/scripts-navigation.js')
    script(src='/contrib/js/scripts-global.js', type='text/javascript')
